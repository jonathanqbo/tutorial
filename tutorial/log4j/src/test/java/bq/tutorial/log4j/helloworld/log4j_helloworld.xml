<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout>
        <pattern>%d{ISO8601} %p %C %m%n</pattern>
      </PatternLayout>
    </Console>
  </appenders>
  <loggers>
    <root level="all">
      <AppenderRef ref="Console"/>
    </root>
  </loggers>
</configuration>